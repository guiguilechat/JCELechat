		<module>SDE-Types-compiler</module>
	</modules>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.helger</groupId>
				<artifactId>jcodemodel</artifactId>
				<version>3.3.0</version>
			</dependency>
			<dependency>
