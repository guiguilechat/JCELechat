<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="800.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.guiguilechat.jcelechat.programs.oreworth.OreWorthController">
   <top>
      <ChoiceBox fx:id="regionSelect" minWidth="-Infinity" BorderPane.alignment="TOP_CENTER" />
   </top>
   <center>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="orename" editable="false" prefWidth="75.0" sortType="DESCENDING" text="ore" />
          <TableColumn fx:id="oreso" editable="false" prefWidth="75.0" sortType="DESCENDING" text="SO" />
            <TableColumn fx:id="oreavg" editable="false" prefWidth="75.0" sortType="DESCENDING" text="avg" />
            <TableColumn fx:id="orebo" editable="false" prefWidth="75.0" sortType="DESCENDING" text="BO" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
